<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Our Team | aua framework</title>
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/2.0.11/typed.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/jquery.waypoints.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/TeamStyle.css" rel="stylesheet" />
</head>
<body>
    <!-- Navbar -->

    <div class="scroll-up-btn">
        <i class="fas fa-angle-up"></i>
    </div>
    <nav class="navbar">
        <div class="max-width">
            <div class="logo">
                <a href="#">aua<span>framework.</span></a>
            </div>
            <ul class="menu">
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">Home</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">About aua</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">Features</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">Documents</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Team">Team</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Price">Pricing</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">Clients</a></li>
                <li><a class="menu-btn" asp-controller="Website" asp-action="Index">Contact</a></li>
            </ul>
            <div class="menu-btn">
                <i class="fas fa-bars"></i>
            </div>
        </div>
    </nav>

    <!-- home section start -->

    <section class="home" id="home">
        <div class="max-width">
            <div class="home-content">
                    <div class="text-1">this is the</div>
                    <div class="text-2">aua's team</div>
                    <div class="text-3">and <span class="typing-team"></span></div>
                    <a href="#contact">Download Demo Version</a>
                </div>
        </div>
    </section>

    @RenderBody()
    <footer>
        <span>
            Designed and Developed By <a href="#">Shahab Attarnejad</a> |
            <span class="far fa-copyright"></span> 2020 All rights reserved.
        </span>
    </footer>

    <script src="~/js/team.js"></script>
    @*<script src="~/js/app.js"></script>*@
</body>
</html>
@RenderSection("scripts", required: false)
