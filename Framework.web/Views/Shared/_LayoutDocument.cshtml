                            <a>
                                <span class="icon"><i class="fa fa-share-alt"></i></span>
                                <span class="list acc-btn">
                                    Access Levels
                                    <span class="fas fa-caret-down sixth"></span>
                                </span>
                            </a>
                            <ul class="acc-show">
                                <li>
                                    <a asp-controller="Document" asp-action="">
                                        <span class="list">WebAuthorize</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="Document" asp-action="">
                                        <span class="list">AllowLoggedInUser</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="Document" asp-action="">
                                        <span class="list">OnlyLocalActionAuthorize</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="Document" asp-action="">
                                        <span class="list">AllowAnonymousAuthorize</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="Document" asp-action="Schema">
                                        <span class="list">OnlyHasAccessToken</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a>
                                <span class="icon"><i class="fas fa-chart-pie"></i></span>
                                <span class="list ope-btn">
                                    Reporting
                                    <span class="fas fa-caret-down first"></span>
                                </span>
                            </a>
                            <ul class="ope-show">
                                <li>
                                    <a asp-controller="Document" asp-action="Index">
                                        <span class="list">Operational Report</span>
                                    </a>
                                </li>
