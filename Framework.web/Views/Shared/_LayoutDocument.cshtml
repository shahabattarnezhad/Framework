                            <a asp-controller="Document" asp-action="WebApi">
                                <span class="icon"><i class="fa fa-cloud"></i></span>
                                <span class="list">Web API</span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Document" asp-action="UI">
                                <span class="icon"><i class="fa fa-paint-brush"></i></span>
                                <span class="list"> UI and UX </span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Document" asp-action="MessageHandler">
                                <span class="icon"><i class="fa fa-envelope"></i></span>
                                <span class="list">Message Provider</span>
                            </a>
                        </li>
