        <div class="top_navbar">
            <div class="logo">
                <a>aua document</a>
            </div>
            <div class="top_menu">
                <div class="home_link">
                    <a asp-controller="Website" asp-action="Index">
                        <span class="icon"><i class="fas fa-home"></i></span>
                        <span>back to home page</span>
                    </a>
                </div>
                <div class="right_info"></div>
            </div>
        </div>

        <div class="main_body">
