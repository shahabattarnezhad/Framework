                            <a asp-controller="Document" asp-action="GitRepository">
                                <span class="icon"><i class="fab fa-github" aria-hidden="true"></i></span>
                                <span class="list">Git Repository</span>
                            </a>
                        </li>
                        <li>
                            <a asp-controller="Document" asp-action="Versions">
                                <span class="icon"><i class="fa fa-barcode"></i></span>
                                <span class="list">Versions</span>
                            </a>
                        </li>
